config_version=5

[application]

config/name="Friday Night Funkin'"
config/tags=PackedStringArray("2d", "modding", "rhythm")
run/main_scene="res://Scenes/Title Screen.tscn"
config/features=PackedStringArray("4.1")
run/delta_smoothing=false
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://Assets/Images/iconOG.png"
config/windows_native_icon="res://Assets/Images/icon.ico"

[autoload]

Settings="*res://Scripts/Autoload/Settings.gd"
Scores="*res://Scripts/Autoload/Scores.gd"
Globals="*res://Scripts/Autoload/Globals.gd"
Paths="*res://Scripts/Autoload/Paths.gd"
Transition="*res://Scenes/Transition.tscn"
Scenes="*res://Scripts/Autoload/SceneHandler.gd"
AudioHandler="*res://Scenes/Audio Handler.tscn"
Conductor="*res://Scripts/Autoload/Conductor.gd"
VolumeSelector="*res://Scenes/Volume Selector.tscn"
FPS="*res://Scenes/FPS.tscn"
PauseMenu="*res://Scenes/Pause Menu.tscn"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"
window/vsync/vsync_mode=false

[editor_plugins]

enabled=PackedStringArray()

[global]

mouse=false

